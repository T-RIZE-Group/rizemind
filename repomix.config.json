{
  "ignore": [
    "**/lib/**",
    "**/out/**",
    "**/node_modules/**",
    "**/.venv/**",
    "**/.pytest_cache/**",
    "**/cache/**",
    "**/*.lock",
    "**/package-lock.json",
    "**/uv.lock",
    "**/repomix-output.xml",
    "**/.DS_Store",
    "**/__pycache__/**",
    "**/*.pyc",
    "**/*.pyo",
    "**/*.pyd",
    "**/.Python",
    "**/build/**",
    "**/develop-eggs/**",
    "**/dist/**",
    "**/downloads/**",
    "**/eggs/**",
    "**/.eggs/**",
    "**/sdist/**",
    "**/wheels/**",
    "**/*.egg-info/**",
    "**/.installed.cfg",
    "**/*.egg",
    "**/.git/**",
    "**/.github/**",
    "**/sphinx/**",
    "**/forge/**"
  ],
  "include": [
    "**/*.py",
    "**/*.sol",
    "**/*.js",
    "**/*.ts",
    "**/*.md",
    "**/*.toml",
    "**/*.json",
    "**/*.rst",
    "**/*.txt",
    "**/*.yml",
    "**/*.yaml"
  ]
}
